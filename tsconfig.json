{
  "compilerOptions": {
    "sourceMap": true,
    "module": "ES6",
    "target": "es5",
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "downlevelIteration": true
  },
  "include": [
    "./src/**/*.tsx",
    "./src/**/*.ts"
  ]
}